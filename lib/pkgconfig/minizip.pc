prefix=/spring-static-libs
exec_prefix=/spring-static-libs
libdir=/spring-static-libs/lib
sharedlibdir=/spring-static-libs/lib
includedir=/spring-static-libs/include

Name: minizip
Description: Minizip zip file manipulation library
Version: 3.0.7

Requires: zlib
Libs: -L${libdir} -L${sharedlibdir} -lminizip
Libs.private: -lz -llzma
Cflags: -I${includedir}
